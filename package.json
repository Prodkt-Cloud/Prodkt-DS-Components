{
  "name": "@prodkt/moyo-components",
  "description": "A labor of love by Tovuti Designers with no Org Support",
  "version": "1.0.13",
  "homepage": "https://github.com/Prodkt-Cloud/moyo-components.git",
  "author": "Bryan Funk",
  "license": "MIT",
  "main": "dist/tovuti.js",
  "module": "dist/tovuti.js",
  "customElements": "dist/custom-elements.json",
  "web-types": "dist/web-types.json",
  "type": "module",
  "types": "dist/tovuti.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "web components",
    "custom elements",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Prodkt-Cloud/moyo-components.git"
  },
  "bugs": {
    "url": "https://github.com/Prodkt-Cloud/moyo-components/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/tovutifunk"
  },
  "scripts": {
    "start": "node scripts/build.js --bundle --serve",
    "clean": "rimraf my-element.{d.ts,d.ts.map,js,js.map} test/my-element.{d.ts,d.ts.map,js,js.map} test/my-element_test.{d.ts,d.ts.map,js,js.map}",
    "build": "node scripts/build.js --bundle --types --copydir \"docs/dist\"",
    "verify": "npm run prettier:check && npm run lint && npm run build && npm run test",
    "prepublishOnly": "npm run verify",
    "prettier": "prettier --write --loglevel warn .",
    "prettier:check": "prettier --check --loglevel warn .",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --max-warnings 0 --fix",
    "ts-check": "tsc --noEmit --project ./tsconfig.json",
    "create": "plop --plopfile scripts/plop/plopfile.js",
    "test": "web-test-runner",
    "test:component": "npm run test -- --group",
    "test:watch": "web-test-runner --watch",
    "spellcheck": "cspell \"**/*.{js,ts,json,html,css,md}\" --no-progress",
    "list-outdated-dependencies": "npm-check-updates --format repo --peer",
    "update-dependencies": "npm-check-updates --peer -u && npm install && npm run lint:fix && npm run prettier && npm run verify",
    "publishnpm": "npm publish --access public",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "@floating-ui/dom": "^0.5.3",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/pro-duotone-svg-icons": "^6.1.1",
    "@fortawesome/pro-light-svg-icons": "^6.1.1",
    "@fortawesome/pro-regular-svg-icons": "^6.1.1",
    "@fortawesome/pro-solid-svg-icons": "^6.1.1",
    "@fortawesome/pro-thin-svg-icons": "^6.1.1",
    "@lit-labs/react": "^1.0.5",
    "@prodkt/animations": "^1.3.0",
    "@prodkt/localize": "^2.1.5",
    "color": "4.2",
    "lit": "^2.2.6",
    "qr-creator": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@open-wc/testing": "^3.1.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/web-components": "^6.5.9",
    "@types/color": "^3.0.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@web/dev-server-esbuild": "^0.3.0",
    "@web/test-runner": "^0.13.31",
    "@web/test-runner-commands": "^0.6.3",
    "@web/test-runner-playwright": "^0.8.9",
    "babel-loader": "^8.2.5",
    "bootstrap-icons": "^1.8.3",
    "browser-sync": "^2.27.10",
    "chalk": "^5.0.1",
    "command-line-args": "^5.2.1",
    "comment-parser": "^1.3.1",
    "cspell": "^6.1.2",
    "del": "^6.1.1",
    "download": "^8.0.0",
    "esbuild": "^0.14.47",
    "eslint": "^8.18.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-lit-a11y": "^2.2.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-wc": "^1.3.2",
    "front-matter": "^4.0.2",
    "get-port": "^6.1.2",
    "globby": "^13.1.2",
    "husky": "^8.0.1",
    "jsonata": "^1.8.6",
    "lint-staged": "^13.0.2",
    "lit-html": "^2.2.6",
    "lunr": "^2.3.9",
    "npm-check-updates": "^14.0.1",
    "open": "^8.4.0",
    "pascal-case": "^3.1.2",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "recursive-copy": "^2.0.14",
    "sinon": "^14.0.0",
    "strip-css-comments": "^5.0.0",
    "tailwindcss": "^3.1.4",
    "tslib": "^2.4.0",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --max-warnings 0 --cache --fix",
      "prettier --write"
    ]
  }
}
